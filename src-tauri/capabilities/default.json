{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default"
  ],
  "fs": {
    "all": {
      "read": true,
      "write": true,
      "copy": true,
      "createDir": true,
      "remove": true,
      "rename": true
    }
  }
}
